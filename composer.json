{
	"name": "tiki/tiki",
	"license": "LGPL-2.1",
	"homepage": "https://tiki.org",
	"minimum-stability": "stable",
	"require": {
		"adodb/adodb": "5.18",
		"bombayworks/zendframework1": "1.12.3",
		"ckeditor/ckeditor": "4.0.3",
		"ezyang/htmlpurifier": "4.5.0",
		"jquery/jquery-sheet": "2.0.1",
		"jquery/jquery-ui": "1.10.2",
		"jquery/jquery-ui-themes": "1.10.2",
		"jquery/jquery-timepicker-addon": "1.2.2",
		"jquery/jquery-ui-selectmenu": "1.3.999",
		"phpcas/phpcas": "1.3.2",
		"phpseclib/phpseclib": "0.3.1",
		"smarty/smarty": "3.1.13",
		"symfony/console": "2.2.0",
		"zetacomponents/base": "1.8",
		"zetacomponents/webdav": "1.1.3"
	},
	"require-dev": {
		"mikey179/vfsStream": "1.1.0"
	},
	"config": {
		"process-timeout": 5000
	},
	"autoload": {
		"psr-0": {
			"Horde": "lib/core/",
			"TikiFilter": "lib/core/",
			"DeclFilter": "lib/core/",
			"JisonParser": "lib/core/",
			"JitFilter": "lib/core/",
			"Search": "lib/core/",
			"Perms": "lib/core/",
			"Math": "lib/core/",
			"Category": "lib/core/",
			"WikiParser": "lib/core/",
			"Multilingual": "lib/core/",
			"Tracker": "lib/core/",
			"Event": "lib/core/",
			"Services": "lib/core/",
			"Tiki": "lib/core/",
			"TikiDb": "lib/core/",
			"Report": "lib/core/",
			"Feed": "lib/core/",
			"FileGallery": "lib/core/",
			"WikiPlugin": "lib/core/"
		},
		"classmap": [
			"lib/pear/",
			"lib/openpgp/",
			"lib/language/",
			"lib/TikiWebdav/",
			"lib/rating/formula/",
			"lib/core/StandardAnalyzer/",
			"vendor/smarty/smarty/distribution/libs/sysplugins/smarty_security.php",
			"vendor/adodb/adodb/",
			"vendor/phpcas/phpcas/",
			"installer/",
			"lib/test"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"Tiki\\Composer\\CleanVendors::clean"
		],
		"post-update-cmd": [
			"Tiki\\Composer\\CleanVendors::clean"
		]
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "phpcas/phpcas",
				"version": "1.3.2",
				"dist": {
					"url": "http://downloads.jasig.org/cas-clients/php/1.3.2/CAS-1.3.2.tgz",
					"type": "tar"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "adodb/adodb",
				"version": "5.18",
				"dist": {
					"url": "http://downloads.sourceforge.net/project/adodb/adodb-php5-only/adodb-518-for-php5/adodb518a.tgz",
					"type": "tar"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "ckeditor/ckeditor",
				"version": "4.0.3",
				"dist": {
					"url": "http://download.cksource.com/CKEditor/CKEditor/CKEditor%204.0.3/ckeditor_4.0.3_standard.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jquery/jquery-sheet",
				"version": "2.0.1",
				"dist": {
					"url": "http://jquerysheet.googlecode.com/files/jquery.sheet-2.0.1.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jquery/jquery-ui",
				"version": "1.10.2",
				"dist": {
					"url": "http://jqueryui.com/resources/download/jquery-ui-1.10.2.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jquery/jquery-ui-themes",
				"version": "1.10.2",
				"dist": {
					"url": "http://jqueryui.com/resources/download/jquery-ui-themes-1.10.2.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jquery/jquery-timepicker-addon",
				"version": "1.2.2",
				"dist": {
					"url": "http://github.com/trentrichardson/jQuery-Timepicker-Addon/archive/v1.2.2.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jquery/jquery-ui-selectmenu",
				"version": "1.3.999",
				"dist": {
					"url": "https://github.com/fnagel/jquery-ui/archive/selectmenu.zip",
					"type": "zip"
				}
			}
		}
	]
}
