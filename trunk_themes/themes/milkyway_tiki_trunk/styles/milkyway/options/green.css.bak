/* $Id: green.css 33678 2011-03-27 03:42:21Z chibaguy $ - Green theme option for Milkyway */
body {background: #346b17}
a:link, a:visited, .cssmenu_vert li a, .cssmenu_vert li a:visited {color: #4ba123}
a:hover {color: #316b16}
table#main {background: #fff url(green/borderl.png) repeat-y;}
table#main > tbody {background: url(green/borderr.png) repeat-y 100% 0;}
td#leftborder {background: url(green/borderl.png) repeat-y}
td#rightborder {background: url(green/borderr.png) repeat-y}
td#footer_td {background: url(green/footer_bottom.jpg) no-repeat 100% 100%;}
#footer {background: url(green/footer_bottom.jpg) no-repeat 0 100%;}
.footer_liner {background: url(green/footer_top.jpg) no-repeat 100% 0;}
.footerbgtrap {background: url(green/footer_top.jpg) no-repeat 0 0;}
#header {background: url(green/header.jpg) no-repeat 100% 0}
#header .wrapper {background: url(green/header.jpg) no-repeat}
.topbar_modules_container {background: url(green/topbar.jpg) no-repeat 100% 0}
.topbar_modules_container > .wrapper {background: url(green/topbar.jpg) no-repeat;}
#Milkyway_topbar_menu .cssmenu_horiz li.menuLevel0 a:hover {background: url(green/mw_menu_active_bg.png) 0 0 repeat-x;color: #fff}
.bt {background: url(green/modbox.jpg) no-repeat 100% 0}
.bt div {background: url(green/modbox.jpg) no-repeat 0 0}
.bb {background: url(green/modbox.jpg) no-repeat 100% 100%}
.bb div {background: url(green/modbox.jpg) no-repeat 0 100%}
.i1 {background: url(green/modboxl.jpg) repeat-y 0 0}
.i2 {background: url(green/modboxr.jpg) repeat-y 100% 0}
.box-titlebottom {background: url(green/modboxtitlebottom.jpg) no-repeat 100% 0}
.box-titlebottom div {background: url(green/modboxtitlebottom.jpg) no-repeat 0 0}

h3.box-title,
span.highlight {
	background: #4ba027;
	color: #fff}

.sf-sub-indicator {background: url(green/arrows-2e6314.png) no-repeat -10px -100px}

.button a:hover,
button:hover,
input[type="reset"]:hover,
input[type="submit"]:hover,
.button input:hover,
.submitbutton:hover,
.calbuttonBox:hover {
	background: /*#2b2b2b*/ url(green/mw_menu_active_bg.png) 0 0 repeat-x;
	color: #fff }

.adminanchors,
th,
td.heading,
tr.diffheader,
th a:link,
th a:visited,
td.heading a:link,
td.heading a:visited,
tr.diffheader a:link,
tr.diffheader a:visited {
	background: #4ba027;
	color: #fff
}

.tabmark a:hover,
.tabactive a:link,
.tabactive a:visited {
	background: url(green/mw_menu_active_bg.png) 0 0 repeat-x;
	color: #fff;
	text-decoration: none;
}