{
	"name": "tiki/tiki",
	"license": "LGPL-2.1",
	"homepage": "https://tiki.org",
	"minimum-stability": "stable",
	"require": {
		"adodb/adodb": "5.18",
		"bombayworks/zendframework1": "1.12.3",
		"ezyang/htmlpurifier": "4.5.0",
		"kriswallsmith/assetic": "1.0.4",
		"leafo/lessphp": "0.3.9",
		"phpcas/phpcas": "1.3.1",
		"phpseclib/phpseclib": "0.3.1",
		"smarty/smarty": "3.1.13",
		"symfony/console": "2.2.0",
		"zetacomponents/base": "1.8",
		"zetacomponents/webdav": "1.1.3"
	},
	"require-dev": {
		"mikey179/vfsStream": "1.1.0"
	},
	"config": {
		"process-timeout": 5000
	},
	"autoload": {
		"psr-0": {
			"Horde": "lib/core/",
			"TikiFilter": "lib/core/",
			"DeclFilter": "lib/core/",
			"JisonParser": "lib/core/",
			"JitFilter": "lib/core/",
			"Search": "lib/core/",
			"Perms": "lib/core/",
			"Math": "lib/core/",
			"Category": "lib/core/",
			"WikiParser": "lib/core/",
			"Multilingual": "lib/core/",
			"Tracker": "lib/core/",
			"Event": "lib/core/",
			"Services": "lib/core/",
			"Tiki": "lib/core/",
			"TikiDb": "lib/core/",
			"Report": "lib/core/",
			"Feed": "lib/core/",
			"FileGallery": "lib/core/",
			"WikiPlugin": "lib/core/"
		},
		"classmap": [
			"lib/pear/",
			"lib/openpgp/",
			"lib/language/",
			"lib/TikiWebdav/",
			"lib/rating/formula/",
			"lib/core/StandardAnalyzer/",
			"vendor/smarty/smarty/distribution/libs/sysplugins/smarty_security.php",
			"vendor/adodb/adodb/",
			"vendor/phpcas/phpcas/",
			"installer/",
			"lib/test"
		]
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "phpcas/phpcas",
				"version": "1.3.1",
				"dist": {
					"url": "http://downloads.jasig.org/cas-clients/php/1.3.1/CAS-1.3.1.tgz",
					"type": "tar"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "adodb/adodb",
				"version": "5.18",
				"dist": {
					"url": "http://downloads.sourceforge.net/project/adodb/adodb-php5-only/adodb-518-for-php5/adodb518a.tgz",
					"type": "tar"
				}
			}
		}
	]
}
