{
	"name": "tiki/tiki",
	"license": "LGPL-2.1",
	"homepage": "https://tiki.org",
	"minimum-stability": "stable",
	"require": {
		"adodb/adodb": "5.18",
		"phpcas/phpcas": "1.3.1",
		"phpseclib/phpseclib": "0.3.1",
		"smarty/smarty": "3.1.13",
		"zetacomponents/base": "1.8",
		"zetacomponents/webdav": "1.1.3",
		"zendframework/zend-filter": "2.0.7",
		"zendframework/zend-mail": "2.0.7",
		"zendframework/zend-i18n": "2.0.7",
		"zendframework/zend-mime": "2.0.7",
		"zendframework/zend-servicemanager": "2.0.7",
		"zendframework/zend-validator": "2.0.7",
		"lphuberdeau/zendsearch": "dev-master",
		"zendframework/zendframework1": "1.12.1"
	},
	"require-dev": {
	},
	"config": {
		"process-timeout": 5000
	},
	"autoload": {
		"psr-0": {
			"Horde": "lib/core/",
			"TikiFilter": "lib/core/",
			"DeclFilter": "lib/core/",
			"JisonParser": "lib/core/",
			"JitFilter": "lib/core/",
			"Search": "lib/core/",
			"Perms": "lib/core/",
			"Math": "lib/core/",
			"Category": "lib/core/",
			"WikiParser": "lib/core/",
			"Multilingual": "lib/core/",
			"Tracker": "lib/core/",
			"Event": "lib/core/",
			"Services": "lib/core/",
			"Tiki": "lib/core/",
			"TikiDb": "lib/core/",
			"Report": "lib/core/",
			"Feed": "lib/core/",
			"FileGallery": "lib/core/",
			"WikiPlugin": "lib/core/"
		},
		"classmap": [
			"lib/pear/",
			"lib/openpgp/",
			"lib/language/",
			"lib/TikiWebdav/",
			"lib/rating/formula/",
			"lib/core/StandardAnalyzer/",
			"lib/webmail/tikimaillib.php",
			"vendor/smarty/smarty/distribution/libs/sysplugins/smarty_security.php",
			"vendor/adodb/adodb/",
			"vendor/phpcas/phpcas/"
		]
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/lphuberdeau/ZendSearch"
		},
		{
			"type": "package",
			"package": {
				"name": "phpcas/phpcas",
				"version": "1.3.1",
				"dist": {
					"url": "http://downloads.jasig.org/cas-clients/php/1.3.1/CAS-1.3.1.tgz",
					"type": "tar"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "adodb/adodb",
				"version": "5.18",
				"dist": {
					"url": "http://downloads.sourceforge.net/project/adodb/adodb-php5-only/adodb-518-for-php5/adodb518a.tgz",
					"type": "tar"
				}
			}
		}
	]
}
